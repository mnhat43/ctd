Program Example2; (* Factorial *)

Var n: Integer, m: String;
Function F(n : Integer) : String;
  Begin
    If n = 0 Then F := 1 Else F := N * F (N - 1);

  End;
  // hello

Begin
  For n := 1 To 7 Do
    Begin
      m = "aaa"; //hello
      m = 7 % n + "cc";
      Call WriteLn;
      Call WriteI( F(m));
    End;
End. (* Factorial *)
